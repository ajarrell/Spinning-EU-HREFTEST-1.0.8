{"components/common/navigation-tools":"<ul class=\"navTools\">\n    <li class=\"navTools-item\">\n        <a class=\"navUser-action navUser-action--quickSearch\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\" data-prevent-quick-search-close=\"true\">\n            <i class=\"fa fa-search\"></i>\n            <div class=\"dropdown dropdown--quickSearch quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n                {{> components/common/quick-search}}\n            </div>\n        </a>\n    </li>\n    <li class=\"navTools-item\">\n        <a class=\"navUser-action\" data-cart-preview data-dropdown=\"cart-preview-dropdown\" data-options=\"align:right\" href=\"{{urls.cart}}\">\n            <i class=\"fa fa-shopping-cart\"></i>\n            <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">\n                {{cart.quantity}}\n            </span>\n        </a>\n        <!--<div class=\"dropdown-menu\" id=\"cart-preview-dropdown{{#if duplicate}}&#45;&#45;duplicate{{/if}}\" data-dropdown-content aria-hidden=\"true\"></div>-->\n    </li>\n</ul>","components/common/quick-search":"{{{ snippet \"forms_search\"}}}\n<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input search_query\" data-search-quick name=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n        </div>\n    </fieldset>\n</form>\n<section class=\"quickSearchResults\" data-bind=\"html: results\"></section>"}
