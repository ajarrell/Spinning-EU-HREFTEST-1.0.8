{"components/above-the-fold/home":"<div class=\"above-the-fold\">\n    {{#if carousel.slides.length}}\n        {{> components/carousel }}\n    {{/if}}\n</div>\n","components/carousel":"<section class=\"heroCarousel\"\n    data-slick='{\n        \"dots\": true,\n        \"mobileFirst\": true,\n        \"slidesToShow\": 1,\n        \"slidesToScroll\": 1,\n        \"autoplay\": true,\n        \"autoplaySpeed\": {{carousel.swap_frequency}},\n        \"adaptiveHeight\": false\n    }'\n>\n    {{#each carousel.slides}}\n    <div class=\"heroCarousel-slide\" style=\"background-image: url({{image}})\">\n        <img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\"/>\n        {{#if heading}}\n            {{> components/carousel-content show_background=true}}\n        {{else}}\n            {{#if text}}\n                {{> components/carousel-content show_background=true}}\n            {{else}}\n                {{#if button_text}}\n                    {{> components/carousel-content show_background=false}}\n                {{/if}}\n            {{/if}}\n        {{/if}}\n    </div>\n    {{/each}}\n</section>\n","components/carousel-content":"<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h1 class=\"heroCarousel-title\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description\">{{{text}}}</p>\n    {{#if button_text}}\n        <a href=\"{{url}}\"><span class=\"heroCarousel-action btn btn-primary btn-rounded-corners\">{{button_text}}</span></a>\n    {{/if}}\n</div>\n"}
